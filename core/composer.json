{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-exif": "*",
        "barryvdh/laravel-ide-helper": "^2.12",
        "beyondcode/laravel-websockets": "^1.13",
        "guzzlehttp/guzzle": "^7.2",
        "hans-thomas/alicia": "^9.0",
        "hans-thomas/horus": "^9.0",
        "hans-thomas/kollector": "^9.0",
        "hans-thomas/pollux": "^9.0",
        "hans-thomas/sphinx": "^9.0",
        "josiasmontag/laravel-recaptchav3": "^1.0",
        "kutia-software-company/larafirebase": "dev-master",
        "laravel/framework": "^9.2",
        "laravel/horizon": "^5.9",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^5",
        "spatie/laravel-activitylog": "^4.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/functions.php",
            "app/Helpers/arrays.php",
            "app/Helpers/constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -M -p",
            "@php artisan ide-helper:eloquent"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "fresh:db": "@php artisan migrate:fresh --seed",
        "test": "@php artisan test",
        "tinker": "@php artisan tinker",
        "unit": "./vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
